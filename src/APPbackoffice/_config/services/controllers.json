{
  "controller.backoffice.account": {
    "extends": "controller.abstract",
    "class": "APPbackofficeControllersAccountController"
  },
  "controller.backoffice.oauth": {
    "extends": "controller.abstract",
    "class": "APPbackofficeControllersOAuthController"
  },
  "controller.backoffice.config": {
    "extends": "controller.abstract",
    "class": "APPbackofficeControllersConfigController"
  },
  "controller.backoffice.eligible_amounts": {
    "extends": "controller.abstract",
    "class": "APPbackofficeControllersEligibleAmountsController"
  },
  "controller.backoffice.translations": {
    "extends": "controller.abstract",
    "class": "APPbackofficeControllersTranslationsController"
  },
  "controller.backoffice.buttons": {
    "extends": "controller.abstract",
    "class": "APPbackofficeControllersButtonsController",
    "arguments": ["@manager.button"]
  },
  "controller.backoffice.system": {
    "extends": "controller.abstract",
    "class": "APPbackofficeControllersSystemController"
  },
  "controller.backoffice.shop": {
    "extends": "controller.abstract",
    "class": "APPbackofficeControllersShopController"
  },
  "controller.backoffice.error": {
    "extends": "controller.abstract",
    "class": "APPbackofficeControllersErrorController"
  },
  "controller.backoffice.diagnostic": {
    "extends": "controller.abstract",
    "class": "APPbackofficeControllersDiagnosticController",
    "calls": [
      {
        "method": "setDiagnosticHandler",
        "arguments": ["@handler.diagnostic"]
      }
    ]
  },
  "controller.backoffice.support": {
    "extends": "controller.abstract",
    "class": "APPbackofficeControllersSupportController"
  },
  "controller.backoffice.logs": {
    "extends": "controller.abstract",
    "class": "APPbackofficeControllersLogsController"
  }
}